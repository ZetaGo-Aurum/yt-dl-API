{
  "name": "youtube-scraper-ytdl",
  "version": "1.0.0",
  "description": "YouTube Scraper API using yt-dlp without YouTube API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "curl -L https://github.com/yt-dlp/yt-dlp/releases/latest/download/yt-dlp -o ./yt-dlp && chmod +x ./yt-dlp"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2"
  },
  "engines": {
    "node": "18.x"
  }
}
